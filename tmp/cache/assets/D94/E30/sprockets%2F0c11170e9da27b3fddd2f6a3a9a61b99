FI"3(function() {

  jQuery(function() {
    var services;
    services = '';
    return $(".order_services").bind("DOMSubtreeModified", function() {
      $('.service_select').last().parent().hide();
      if (services === '') {
        services = $('.service_select').last().html();
      }
      return $(".category_select").change(function() {
        var cat_sel, category, category_selector, escaped_category, options;
        category_selector = this;
        category = $(':selected', category_selector).text();
        escaped_category = category.replace(/([#;&,.+*~\':"!^$[\]()=>|\/@])/g, '\\$1');
        options = $(services).filter("optgroup[label='" + escaped_category + "']").html();
        cat_sel = $(category_selector).parent().parent();
        if (options) {
          $('.service_select', cat_sel).html(options);
          return $('.service_select', cat_sel).parent().show();
        } else {
          $('.service_select', cat_sel).empty();
          return $('.service_select', cat_sel).parent().hide();
        }
      });
    });
  });

}).call(this);
